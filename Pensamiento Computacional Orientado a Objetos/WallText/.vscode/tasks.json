{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: cl.exe build active file",
			"command": "C:\\MinGW\\bin\\gcc.exe",
			"args": [
				"-g",
				"${fileDirname}\\${fileBasenameNoExtension}.exe",
				"-o",
				"${file}"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
		}
	]
}